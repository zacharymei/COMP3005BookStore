<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Order Form</title>

    <link rel="stylesheet" href="../css/userPanel.css">
    <link rel="stylesheet" href="../css/orderform.css">


    <script type="text/javascript" src="../scripts/userPanel.js"></script>

  </head>
  <body>

    <%- include("../partials/user")%>

    <div class="main" id="main">
      <button type="button" name="button" id="toggleUser" onclick="closeUser()"><i class="material-icons">menu</i></button>
      <% if(data.session.loggedin){ %>
        <%- include("../partials/loginHeader") %>
      <% } else { %>
        <%- include("../partials/header") %>
      <% } %>

      <%- include("../partials/header") %>


      <div class="dataPanel">
        <body onload="init()">
          <select id="restaurant-select"></select>
          <br><br>
          <div id="info"></div>
          <div id="menu">
            <div class="column left" id="left"></div>
            <div class="column middle" id="middle"></div>
            <div class="column right" id="right"></div>
          </div>

          <script src="../scripts/orderform.js"></script>
      </div>


    </div>

  </body>
</html>
